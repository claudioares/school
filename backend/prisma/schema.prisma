// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Student {
  id         String    @id @default(uuid())
  student    String
  email      String    @unique
  password   String
  birth      DateTime?
  nameMother String?
  cpf        String?   @unique
  nameFather String?
  endress    String?
  highSchool Boolean?
}
